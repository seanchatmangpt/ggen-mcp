PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX ddd: <http://example.org/ddd#>

SELECT ?name ?type
WHERE {
    ?entity a ?type ;
           rdfs:label ?name .
    FILTER(?type IN (ddd:AggregateRoot, ddd:Command, ddd:ValueObject))
}
ORDER BY ?name
